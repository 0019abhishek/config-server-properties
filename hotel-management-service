server:
  port: 8082

spring:
  application:
    name: hotel-management-service

  data:
    mongodb:
      uri: mongodb://localhost:27017/HotelManagement

  main:
    allow-bean-definition-overriding: true

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://localhost:8761/eureka
  instance:
    prefer-ip-address: true

